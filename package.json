{
  "name": "rock-tracks",
  "version": "1.0.0",
  "description": "View basic rock track details gathered with the itunes search api",
  "repository": "https://github.com/thomas5631/rock-tracks",
  "author": "Thomas Lilley",
  "license": "MIT",
  "scripts": {
    "build": "export NODE_ENV=production && webpack --progress",
